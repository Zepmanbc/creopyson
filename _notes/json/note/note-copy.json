{
  "spec" : {
    "function_description" : "Copy note to another in the same model or another model",
    "command" : "note",
    "function" : "copy",
    "request" : [ {
      "name" : "file",
      "type" : "string",
      "description" : "Source model",
      "default" : "The currently active model",
      "wildcards_allowed" : true
    }, {
      "name" : "name",
      "type" : "string",
      "description" : "Note name to copy",
      "wildcards_allowed" : true
    }, {
      "name" : "to_name",
      "type" : "string",
      "description" : "Destination note",
      "default" : "The source note name"
    }, {
      "name" : "to_file",
      "type" : "string",
      "description" : "Destination model",
      "default" : "The source model"
    } ],
    "response" : [ ]
  },
  "examples" : [ {
    "request" : {
      "sessionId" : "~sessionId~",
      "command" : "note",
      "function" : "copy",
      "data" : {
        "file" : "box.prt",
        "name" : "ALPHA",
        "to_name" : "BETA"
      }
    },
    "response" : {
      "status" : {
        "error" : false
      }
    }
  }, {
    "request" : {
      "sessionId" : "~sessionId~",
      "command" : "note",
      "function" : "copy",
      "data" : {
        "file" : "box_flat.prt",
        "name" : "Note_2*",
        "to_file" : "box.prt"
      }
    },
    "response" : {
      "status" : {
        "error" : false
      }
    }
  } ]
}