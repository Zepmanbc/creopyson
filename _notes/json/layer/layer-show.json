        "name" : "*CSYS"
      }
    },
    "response" : {
      "status" : {
        "error" : false
      }
    }
  }, {
    "request" : {
      "sessionId" : "~sessionId~",
      "command" : "layer",
      "function" : "show",
      "data" : {
        "file" : "*.asm",
        "name" : "*CSYS",
        "show" : false
      }
    },
    "response" : {
      "status" : {
        "error" : false
      }
    }
  } ]
